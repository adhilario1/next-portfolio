    "/gallery": {
      "name": "/gallery",
      "lambdaFunction": "galleryLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/game": {
